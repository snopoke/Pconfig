<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pconfig xmlns="http://www.cell-life.org/schemas/pconfig"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.cell-life.org/schemas/pconfig http://www.cell-life.org/schemas/pconfig/pconfig-1.0.xsd">
	<id>demo_subreport</id>
	<label>Demo report</label>
	<resource>demo_w_subreport.jasper</resource>
	<parameters>
		<date label="Start date" name="startDate" allowPast="true"
			allowFuture="false" />
		<integer label="Integer param" name="integer-param" max="10" />
		<text label="Mobilie number" name="msisdn" regex="^27[1-9][0-9]{8}$"
			errorMessage="This field only accepts numbers of this format: 27821231234" />
		<boolean label="Include test data" name="include-test" />
	</parameters>
</pconfig>
